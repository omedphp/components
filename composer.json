{
    "name": "omed/omed",
    "description": "Omed Component Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Omed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spec\\Omed\\Component\\User\\": "src/Component/User/Spec",
            "Tests\\Omed\\Component\\User\\": "src/Component/User/Tests",
            "Spec\\Omed\\Laravel\\API\\User\\": "src/Laravel/API/User/Spec",
            "Tests\\Omed\\Laravel\\API\\User\\": "src/Laravel/API/User/Tests",
            "Tests\\Omed\\Laravel\\API\\Core\\": "src/Laravel/API/Core/Tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "doctrine/collections": "^1.6",
        "doctrine/orm": "^2.6.6|^2.7",
        "illuminate/auth": "^5.8|^6.0|^7.0",
        "laravel-doctrine/orm": "^1.4|^1.5|^1.6",
        "symfony/security-core": "^3.4|4.0|^5.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpspec/phpspec": "^6.2",
        "phpstan/phpstan": "^0.12.30",
        "phpunit/phpunit": "^8.0|^9.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-phpunit": "^0.12.11"
    }
}
